{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/rubbish-recycling",
      "dest": "/rubbish-recycling.html"
    },
    {
      "src": "/furniture-whiteware",
      "dest": "/furniture-whiteware.html"
    },
    {
      "src": "/green-waste",
      "dest": "/green-waste.html"
    },
    {
      "src": "/commercial-waste",
      "dest": "/commercial-waste.html"
    },
    {
      "src": "/estate-clearance",
      "dest": "/estate-clearance.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

