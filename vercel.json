{
  "rewrites": [
    {
      "source": "/rubbish-recycling",
      "destination": "/rubbish-recycling.html"
    },
    {
      "source": "/furniture-whiteware",
      "destination": "/furniture-whiteware.html"
    },
    {
      "source": "/green-waste",
      "destination": "/green-waste.html"
    },
    {
      "source": "/commercial-waste",
      "destination": "/commercial-waste.html"
    },
    {
      "source": "/estate-clearance",
      "destination": "/estate-clearance.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "no-referrer-when-downgrade"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}

